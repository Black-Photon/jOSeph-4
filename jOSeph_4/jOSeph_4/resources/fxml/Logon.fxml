<GridPane hgap="10.0" prefHeight="160.0" prefWidth="350.0" stylesheets="@../css/main.css" vgap="8.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jOSeph_4.resources.controllers.Logon_Controller">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" maxWidth="119.0" minWidth="66.0" prefWidth="70.0" />
